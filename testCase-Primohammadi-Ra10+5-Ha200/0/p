FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            calculated;
        value           $internalField;
    }

    outlet
    {
        type            calculated;
        value           $internalField;
    }

    frontwall
    {
        type            empty;
    }

    backwall
    {
        type            empty;
    }

    hotwall
    {
        type            calculated;
        value           $internalField;
    }

    coldwall
    {
        type            calculated;
        value           $internalField;
    }

}


// ************************************************************************* //
